{
  "manifest_version": 3,
  "name": "YT-Transcript",
  "description": "Chrome extension to put the transcript of the current youtube link into the clipboard",
  "version": "1.0",
  "action": {
    "default_title": "YT-Transcript Extension",
    "default_icon": "images/icon.png"
  },
  "key": "",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["script.js"]
    }
  ]
}